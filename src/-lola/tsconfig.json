{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "outDir": "./dist",
    "rootDir": "./",
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "./**/*.ts",
    "../../seludom/**/*.ts",
    "../../sepyt/**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}
